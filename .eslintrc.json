{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 2,
    "no-shadow": 0,
    "react-hooks/exhaustive-deps": 0,
    "no-new": 0,
    "jsx-a11y/no-noninteractive-tabindex": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "react/destructuring-assignment": 0,
    "typescript/no-explicit-any": 0,
    "react/prop-types": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "endOfLine": "auto",
        "semi": false
      }
    ]
  }
}